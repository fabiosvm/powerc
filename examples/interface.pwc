
// interface.pwc

interface I {
  fn Void f(Self self);
}

interface J {
  fn Void g(Self self);
}

interface K: I + J {
  fn Void h(Self self);
}

struct S {}

fn Void f(S self) {
  println("f() called");
}

fn Void g(S self) {
  println("g() called");
}

fn Void h(S self) {
  println("h() called");
}

fn Void main() {
  var K k;
  k = new S();
  k.f(); // f() called
  k.g(); // g() called
  k.h(); // h() called
}
