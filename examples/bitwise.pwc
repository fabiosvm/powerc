
// bitwise.pwc

fn Int main() {
  var Int a = 0;
  var Int b = 1;
  println(string_from_int(a | b)); // 1
  println(string_from_int(a ^ b)); // 1
  println(string_from_int(a & b)); // 0
  println(string_from_int(~b)); // -2
  println(string_from_int(b << 1)); // 2
  println(string_from_int(b >> 1)); // 0
}
